# git
alias ga='git add'
alias gs='git status'
alias gc='git commit'
alias gac='git add -u && git commit -m'
alias gd='git diff'
alias gl='git log'
alias gp='git pull'
alias gcb='git checkout -b'
alias gco='git checkout'
alias gf='git fetch'
alias gm='git merge'
alias gph="git push origin HEAD"


# colors
alias ls='ls --color=auto'

